<script>
    <% if(error){ %>
    swal({
        title: "Error!",
        text: '<%= error %>',
        icon: "error",
    });
    <% } %>

    <% if(info){ %>
    swal({
        title: "Success!",
        text: '<%= info %>',
        icon: "success",
    });
    <% } %>
</script>

